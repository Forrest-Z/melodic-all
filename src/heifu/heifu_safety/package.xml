  <maintainer email="fabio.azevedo@beyond-vision.pt">Fábio Azevedo</maintainer>

  <license>BSD</license>
  <url type="repository">https://gitlab.pdmfc.com/drones/heifu</url>
  <author email="fabio.azevedo@beyond-vision.pt">Fábio Azevedo</author>

  <buildtool_depend>catkin</buildtool_depend>

  <build_depend>roscpp</build_depend>
